local playerF = game.Players.LocalPlayer
local playerW = playerF.Name

game:GetService("ReplicatedStorage").Security.RemoteEvent:Destroy() game:GetService("ReplicatedStorage").Security[""]:Destroy()
game:GetService("ReplicatedStorage").Security:Destroy() game:GetService("Players").LocalPlayer.PlayerScripts.Client.DeviceChecker:Destroy()

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Toggle = Instance.new("TextButton")
local ToggleOFF = Instance.new("TextButton")
local ToggleZ = Instance.new("TextButton")
local ToggleZOFF = Instance.new("TextButton")
local aliveFolder = workspace:WaitForChild("Alive")
local ballsFolder = workspace:WaitForChild("Balls")
local run = game:GetService("RunService")
local sim = run.Heartbeat
local sim2 = run.RenderStepped
local camera = workspace.CurrentCamera
local direcaoDaCamera = camera.CFrame.LookVector
local posicaoDaCamera = camera.CFrame
local posicaoDaCamera2 = camera.CFrame.Position
local distancia = 100
local args = {
    [1] = 0.5,
    [2] = posicaoDaCamera + (direcaoDaCamera * distancia),
    [3] = {
        ["58565443542"] = posicaoDaCamera2 + (direcaoDaCamera * distancia),
        ["58365443543"] = posicaoDaCamera2 + (direcaoDaCamera * distancia)
    },
    [4] = {
        [1] = 749,
        [2] = 179.5
    },
    [5] = false
}

local realBall = nil
local realPlayer = nil
local realOther = nil
local targ = nil
local from = nil
local pre = nil
local pre2 = nil
local pre3 = nil
local pre4 = nil
local spam = false
local tg = true
local parry = game:GetService("SocialService"):WaitForChild("\n\n\n\n\n\n\n\n")

print("sucesss run")

