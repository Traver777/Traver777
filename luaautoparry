local function newBall()
realBall = nil
for _, ball in ipairs(ballsFolder:GetChildren()) do
if ball:GetAttribute("realBall") then
realBall = ball
targ = ball:GetAttribute("target")
break
end
end
end

local function newPlay()
realPlayer = nil
for _, ply in ipairs(aliveFolder:GetChildren()) do
if ply.Name == playerW then
realPlayer = ply
rootpart = ply:FindFirstChild("HumanoidRootPart")
break
end
end
end

local function newOther()
realOther = nil
for _, other in ipairs(aliveFolder:GetChildren()) do
if other.Name ~= playerW then
realOther = other
break
end
end
end

local function argsAct()
if realPlayer and realPlayer.Parent then
local rootPos = rootpart.CFrame.Position
args = {
0.5,
rootpart.CFrame * CFrame.new(0, 0, -60),
{ ["42423424423"] = rootPos + Vector3.new(0, 0, -60) },
{ 795.5, 152.5 },
false
}
  end
end

newBall()
newPlay()
newOther()

local function checkNoMiss()
if not (realBall and realPlayer and realOther) then return end
local ballVelocity = realBall.Velocity.Magnitude
local playerTorso = realPlayer.HumanoidRootPart.Position
local opponentTorso = realOther.HumanoidRootPart.Position
local distanceToBall = (playerTorso - realBall.Position).Magnitude
local opponentDistance = (playerTorso - opponentTorso).Magnitude
local adjustedDistance = 22 + (ballVelocity / 2 - distanceToBall / 4.2)
local requiredDistance = distanceToBall <= adjustedDistance
local spamDistance = 30 + (ballVelocity / 7 - distanceToBall / 5)
local safeDistance = 25 + (ballVelocity / 6 - distanceToBall / 5)
if targ ~= playerW then
tg = true
end
if requiredDistance and tg == true then
if targ == playerW then
parry:FireServer(unpack(args))
tg = false
end
end
if targ ~= playerW then
sp = true
end
if distanceToBall <= spamDistance and opponentDistance <= safeDistance then
if sp == true and targ == playerW and contador < 2 then
contador = contador + 1
sp = false
end
else
spam = false
contador = 0
end
if contador >= 2 then spam = true end
end

local function newAll()
newBall()
newPlay()
newOther()
argsAct()
checkNoMiss()
end

local function toggleOn()
pre = sim2:Connect(function()
if realPlayer and realPlayer.Parent then
parry:FireServer(unpack(args))
end
end)
end

local function toggleOff()
pre:Disconnect()
end

local function toggleZ()
pre2 = sim:Connect(newAll)
pre3 = sim2:Connect(function()
if spam == true and realPlayer and realPlayer.Parent and realOther and realOther.Parent then
parry:FireServer(unpack(args))
end
end)
pre4 = sim2:Connect(function()
if spam == true and realPlayer and realPlayer.Parent and realOther and realOther.Parent then
parry:FireServer(unpack(args))
end
end)
end

local function toggleZOFF()
pre2:Disconnect()
pre3:Disconnect()
pre4:Disconnect()
end

Toggle.MouseButton1Click:Connect(toggleOn)
ToggleOFF.MouseButton1Click:Connect(toggleOff)
ToggleZ.MouseButton1Click:Connect(toggleZ)
ToggleZOFF.MouseButton1Click:Connect(toggleZOFF)

Frame.Active = true
Frame.Draggable = true
